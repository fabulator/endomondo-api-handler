{
    "name": "endomondo-api-handler",
    "version": "3.1.1",
    "description": "Unofficial handler for Endomondo API",
    "main": "dist/index.js",
    "jsnext:main": "dist/index.esm.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "lint": "eslint --ext .ts ./src ./tests",
        "lint:export": "npm run lint -- -o ./tests_results/checkstyle/js-checkstyle.xml -f checkstyle",
        "tsc": "tsc",
        "build": "rollup --config ./rollup.config.js",
        "develop": "tsc --emitDeclarationOnly false --watch",
        "test": "jest",
        "test:build": "npm run test -- --config jest.config.build.js",
        "test:all": "npm run lint && npm run tsc && npm run test",
        "prepublishOnly": "npm run build && npm run tsc && npm run test:build",
        "release": "semantic-release"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/fabulator/endomondo-api-handler.git"
    },
    "author": "Michal Ozog√°n <michal@ozogan.eu>",
    "license": "Apache-2.0",
    "homepage": "https://github.com/fabulator/endomondo-api-handler#readme",
    "keywords": [
        "endomondo",
        "endomondo-api"
    ],
    "devDependencies": {
        "@socifi/babel-config": "^0.7.0",
        "@socifi/browserslist-config": "^0.1.0",
        "@socifi/commitlint-config": "^1.0.0",
        "@socifi/eslint-config": "^2.8.0",
        "@socifi/jest-config": "^2.3.1",
        "@socifi/rollup-config": "^2.1.0",
        "cross-fetch": "^3.0.4",
        "husky": "^3.0.8",
        "semantic-release-config-fabulator": "^1.0.3"
    },
    "dependencies": {
        "@types/luxon": "^1.15.2",
        "@types/mathjs": "^6.0.2",
        "cookie-api-handler": "^2.1.0",
        "fitness-models": "^0.4.1",
        "gpx-builder": "^2.0.1",
        "luxon": "^1.19.3",
        "mathjs": "^6.2.3",
        "query-string": "^6.8.3",
        "rest-api-handler": "^2.2.0"
    }
}
