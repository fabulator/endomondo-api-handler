{
  "name": "endomondo-api-handler",
  "version": "1.3.1",
  "description": "Unofficial handler for Endomondo API",
  "main": "./dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "scripts": {
    "lint": "eslint --ext .js ./src ./tests",
    "lint-export": "npm run lint -- -o ./tests_results/checkstyle/js-checkstyle.xml -f checkstyle",
    "flow-init": "flow-typed install cookie@0.3.x jest@22.x query-string@6.x luxon@0.4.x xmlbuilder@x.x",
    "flow": "flow",
    "flow-coverage": "flow-coverage-report -f \"node node_modules/flow-bin/vendor/flow\" -i ./src/**/*.js* -t html -t text -t json -o tests_results/coverage/flow",
    "build": "rm -rf ./dist && rollup --config ./rollup.config.js",
    "test": "jest",
    "test-build": "npm run test -- --config jest.config.build.js",
    "precommit": "npm run lint && npm run flow && npm run test",
    "prepublishOnly": "npm run build && npm run test-build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fabulator/endomondo-api-handler.git"
  },
  "author": "Michal Ozog√°n <michal@ozogan.eu>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/fabulator/endomondo-api-handler#readme",
  "keywords": [
    "endomondo",
    "endomondo-api"
  ],
  "devDependencies": {
    "cross-fetch": "^2.1.1",
    "@socifi/eslint-config": "^1.10.0",
    "flow-bin": "^0.85.0",
    "flow-coverage-report": "^0.6.0",
    "flow-typed": "^2.5.1",
    "@socifi/jest-config": "^1.10.0",
    "@socifi/rollup-config": "^1.10.0"
  },
  "dependencies": {
    "cookie-api-handler": "^1.1.0",
    "gpx-builder": "^1.0.1",
    "luxon": "^1.4.3",
    "mathjs": "^5.2.0",
    "query-string": "^6.2.0",
    "rest-api-handler": "^1.8.1"
  }
}
